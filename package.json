{
  "name": "express-remember",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "momo": "cd src && npx sequelize-cli model:generate --name ",
    "gomigrate": "cd src && npx sequelize-cli db:migrate",
    "destro": "cd src && npx sequelize-cli db:migrate:undo:all",
    "destro:one": "cd src && npx sequelize-cli db:migrate:undo",
    "skeleton": "cd src && npx sequelize-cli migration:generate --name "
  },
  "dependencies": {
    "@hapi/boom": "^9.1.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "joi": "^17.4.2",
    "nodemon": "^2.0.15",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.12.0-beta.1",
    "sequelize-cli": "^6.3.0"
  },
  "engines": {
    "node": "14.18.0"
  },
  "devDependencies": {}
}
